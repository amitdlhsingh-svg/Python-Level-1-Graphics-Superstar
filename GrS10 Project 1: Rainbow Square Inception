import turtle #start
import random
t=turtle.Turtle()
t.speed(0)
t.colormode(255)
t.speed(0)
r=random.randint(0,255)
g=random.randint(0,255)
b=0
for i in range(36): #nested loop
  l=30
  t.color(r,g,b)
  for i in range(15):
    for i in range(4):
      t.forward(l)
      t.right(90)
    l+=20 #add 20 each time
  t.right(10) #end
  b+=20
