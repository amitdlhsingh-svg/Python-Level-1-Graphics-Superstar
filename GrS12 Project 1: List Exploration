import turtle
t=turtle.Turtle()
t.pensize(30)
colors = [] 
colors.append("blue")
colors.append("green")
colors.append("red")
colors.append("purple")
colors.append("yellow")
for color in colors:
  t.color(color)
  t.forward(10)
if "purple" in colors:
  t.write("Yes", font = ("Arial", 100, "normal"))
else:
  t.write("No", font = ("Arial", 100, "normal"))
